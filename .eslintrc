{
  "extends": ["next", "prettier", "plugin:tailwindcss/recommended"],
  "rules": {
    "no-console": "warn",
    "eqeqeq": "warn",
    "tailwindcss/no-custom-classname": "off"
  },
  "settings": {
    "tailwindcss": {
      "callees": ["clsx", "cn"],
      "classRegex": "^(class|\\w+Class)Name$"
    }
  }
}
